{
    "name": "okhachiai/dashify-bundle",
    "description": "A modern, modular and intuitive admin interface for Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dashify\\DashifyBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "KHACHIAI Oussama",
            "email": "oussama.khachiai@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/http-kernel": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/config": "^7.2",
        "symfony/form": "^7.2",
        "symfony/security-bundle": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/asset": "^7.2",
        "symfony/routing": "^7.2",
        "symfony/doctrine-bridge": "^7.2",
        "doctrine/orm": "^2.17",
        "doctrine/doctrine-bundle": "^2.11"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
